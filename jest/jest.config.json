{
  "coverageDirectory": "./coverage",
  "testMatch": ["**/?(*.)+(spec).js"],
  "transformIgnorePatterns": ["node_modules/(?!spacetime)"],
  "transform": {
    "\\.[jt]sx?$": "babel-jest"
  },
  "testPathIgnorePatterns": [
    "/node_modules/"
  ],
  "verbose": true,
  "resetMocks": true,
  "clearMocks": true
}
